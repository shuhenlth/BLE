.div{
  line-height: 60rpx;
  font-weight: bolder;
}

.line_view{
  margin: 20rpx 100rpx 0rpx 100rpx;
  display: flex;
  height: 60rpx;
}

.select{
  display: flex;
  margin: 20rpx 100rpx 0rpx 100rpx;
  height: 60rpx;
}

.input-class {
  margin-left: 30rpx;
  height: 60rpx;
  width: 60%;
  line-height: 60rpx;
  background-color: #eee;
  text-align: center;
  border: 1px solid #ccc;
  border-radius: 8rpx;
  box-sizing: border-box;
}

.parentstyle {
  display: flex;
  align-items: center;
  border: 1rpx solid #e0e0e0;
  border-radius: 10rpx;
  box-shadow: 0 0 5rpx #e0e0e0;
  margin: 30rpx 38px;
  padding: 20rpx;
}

.parentstyle .imageStyle {
  width: 41rpx;
  height: 41rpx;
  margin-right: 20rpx;
}

.parentstyle .centerStyle {
  display: flex;
  flex: 1;
  align-items: center;
  justify-content: space-between;
  font-size: 28rpx;
}

.btn{
  display: flex;
  margin: 30rpx 35rpx 0rpx 35rpx;
  font-family: 'Courier New', Courier, monospace;
}
.saveChanges{
  width: 30%;
  height: 80rpx;
  background-color: rgb(28, 173, 71);
  color: white;
}
.send{
  width: 30%;
  height: 80rpx;
  color: white;
}
.refreshInfo{
  width: 30%;
  height: 80rpx;
  background-color: rgb(223, 65, 139);
  color: white;
}

